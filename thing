									if game.Workspace.Effects.SpawnReward.BillboardGui.Frame.BackgroundColor3 ~= Color3.fromRGB(255, 0, 0) then
										
										Effect.Position = Plr.Character.HumanoidRootPart.Position
										
									end
									
								end

							end

						end)

					else

						Heals = false

					end

				end,

			},
			
		}

	end,
	
	["10449761463"] = function ()

		local Noclipping
		
		local function Noclip()
			
			Noclipping = game["Run Service"].Stepped:Connect(function()
				if Plr.Character ~= nil then
					for _, Child in pairs(Plr.Character:GetDescendants()) do
						if Child:IsA("BasePart") and Child.CanCollide == true then
							Child.CanCollide = false
						end
					end
				end
			end)
			
		end
		
		local function Clip()

			Noclipping:Disconnect()
			
			for _, Child in pairs(Plr.Character:GetDescendants()) do
				if Child:IsA("BasePart") and Child.CanCollide == false then
					Child.CanCollide = true
				end
			end

		end

		local function BringBlock()

			local Block = Instance.new("Part",game.Workspace)
			
			Block.CFrame = Plr.Character.HumanoidRootPart.CFrame
			Block.Size = Vector3.new(1,1,1)
			Block.Transparency = 1
			Block.Anchored = true
			Block.CanCollide = false
			Block.CanTouch = false
			Block.CanQuery = false
			
			spawn(function()
				
				while Block.Parent ~= nil do

					wait()
					Plr.Character.HumanoidRootPart.CFrame = Block.CFrame

				end
				
			end)
			
			return Block

		end
		
		Commands = {

			{

				Type = "Title",
				Text = "Characters"

			},

			{

				Name = "Vortex Edge",
				Desc = 'Is all about air and speed. You need "The Strongest Hero" for this to work.',
				Type = "Button",
				Action = function()

					if not Plr.Character:GetAttribute("Character") or Plr.Character:GetAttribute("Character") ~= "Bald" then
						
						ShowPopup("Invalid Character",'You need "The Strongest Hero" (or Bold in code name) to use this character.',"Oops")
						return
						
					end
					
					Plr.PlayerGui.Hotbar.Backpack.LocalScript.Enabled = false
					
					-- Nullify
					
					game:GetService("Players").LocalPlayer.PlayerGui.Hotbar.Backpack.Hotbar["1"].Base.ToolName.Text = "Nullify"

					local function Nullify()
						
						local ClosestPlayer = nil
						local ClosestDistance = math.huge
						
						for _, Player in game.Workspace.Live:GetChildren() do
							if Player ~= Plr.Character and Player:FindFirstChild("HumanoidRootPart") then
								local Distance = (Player.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
								if Distance < ClosestDistance then
									
									ClosestDistance = Distance
									ClosestPlayer = Player
									
								end
							end
						end
						
						if ClosestDistance > 40 or ClosestPlayer == nil then
							
							local Block = BringBlock()
							
							Noclip()
							game.TweenService:Create(Block,TweenInfo.new(0.5),{CFrame = Block.CFrame - Vector3.new(0,10,0)}):Play()
							wait(0.5)
							game.TweenService:Create(Block,TweenInfo.new(0.5),{CFrame = Block.CFrame + Vector3.new(0,10,0)}):Play()
							wait(0.5)
							Clip()
							Block.Parent = nil
							return
								
						end
						
						local Block = BringBlock()

						Noclip()
						game.TweenService:Create(Block,TweenInfo.new(0.5),{CFrame = Block.CFrame - Vector3.new(0,8,0)}):Play()
						wait(0.5)
						for i=1,100 do
							
							wait()
							
							Block.CFrame = CFrame.new(ClosestPlayer.HumanoidRootPart.Position - Vector3.new(0,8,0)) * CFrame.Angles(math.rad(90),0,0)
							
						end
						Clip()
						Block.CFrame = ClosestPlayer.HumanoidRootPart.CFrame
						wait(0.01)
						Block.Parent = nil
						
					end

					-- Activate
					
					local Pressed1 = Plr.PlayerGui.Hotbar.Backpack.Hotbar["1"].Base.MouseButton1Down:Connect(Nullify)

					local Button1 = game.UserInputService.InputBegan:Connect(function(Key,Chat)

						if Chat then

							return

						end

						if Key.KeyCode == Enum.KeyCode.One then

							Nullify()

						end

					end)

					Plr.Character.Humanoid.Died:Connect(function()

						Button1:Disconnect()
						Pressed1:Disconnect()

					end)
					
				end,

			},

		}

	end,
	
	["70671905624144"] = function ()
		local basepos = Plr.Character.HumanoidRootPart.CFrame
		Commands = {
			
			{

				Type = "Title",
				Text = "CLAAAAAAAAAAAAAAAA"

			},
			{

				Name = "Set Base",
				Desc = 'Click this inside of your base.',
				Type = "Button",
				Action = function()

					basepos = Plr.Character.HumanoidRootPart.CFrame

				end,

			},
			{

				Name = "Quick Steal Button",
				Desc = 'Everytime you try to steal a baddie, you instantly get it and teleport to your base then back.',
				Type = "Button",
				Action = function()

					for _, Item in game:GetDescendants() do
						
						if Item.Name == "StealProximity" and Item:IsA("ProximityPrompt") then
							
							Item.HoldDuration = 0
							Item.RequiresLineOfSight = false
							
							Item.Triggered:Connect(function(trig)
								
								if trig == Plr then
									
									local oldpos = Plr.Character.HumanoidRootPart.CFrame
									
									Plr.Character.HumanoidRootPart.CFrame = basepos
									
									wait(0.3)
									
									Plr.Character.HumanoidRootPart.CFrame = oldpos
									
								end
								
							end)
							
						end
						
					end
					
				end,

			},

		}

	end,
	
