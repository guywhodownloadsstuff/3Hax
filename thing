			if Input.KeyCode == Enum.KeyCode.T and not Chat and not Closed then

				if Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") and game.Players:GetPlayerFromCharacter(Mouse.Target.Parent) then

					ShowPopup(Mouse.Target.Parent:GetFullName(),"You have selected " .. Mouse.Target.Parent.Name .. ".","Nice")
					Target = Mouse.Target.Parent
					return

				else

					if Mouse.Target.Parent.Parent:FindFirstChildOfClass("Humanoid") and game.Players:GetPlayerFromCharacter(Mouse.Target.Parent.Parent) then

						ShowPopup(Mouse.Target.Parent.Parent:GetFullName(),"You have selected " .. Mouse.Target.Parent.Parent.Name .. ".","Nice")
						Target = Mouse.Target.Parent.Parent
						return

					else

						ShowPopup("Error","Invalid target.","Oh")
						Target = nil
						return

