															game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Track

														end

													end)
													Item.CFrame = OldPos2

													break

												end

											end

										end

									end

								end

							end

						end)

					else

						ItemGiving = false

					end

				end,

			},

		}

	end,
	
	["104368835975147"] = function ()

		local Diamonds = false
		local Heals = false
		
		Commands = {

			{

				Type = "Title",
				Text = "Player"

			},

			{

				Name = "Get Diamonds",
