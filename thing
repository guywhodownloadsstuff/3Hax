			{

				Name = "Invisible",
				Type = "Button",
				Desc = "Makes you invisible and have a forcefield, Die to turn this off.",
				Action = function(On)

					if Plr.Character:HasTag("Invisible") then

						ShowPopup("Invisible","Your already invisible!","Oh")
						return

					end

					local Base = nil

					for _, Thing in game.Workspace:GetChildren() do

						if Thing:FindFirstChild("Owner") and Thing:FindFirstChild("Owner"):IsA("StringValue") and Thing:FindFirstChild("Owner").Value == Plr.Name then

							Base = Thing

						end

					end

					local Tap = false
					local Platform = Instance.new('Part')
					Platform.Anchored = true
					Platform.CanCollide = true
					Platform.Size = Vector3.new(10,1,10)
					Platform.Position = Base.SpawnLocation.Position + Vector3.new(0,20,0)
					Platform.Transparency = 1
					Platform.Parent = game.Workspace

					Plr.Character:AddTag("Invisible")

					local Touched = Platform.Touched:connect(function(part)
						if (part.Parent.Name == game.Players.LocalPlayer.Name) then
							if Tap == false then
								Tap = true
								local function Apply()
									local Clone = Plr.Character.HumanoidRootPart:Clone()
									wait(0.25)
									Plr.Character.HumanoidRootPart:Destroy()
									Clone.Parent = Plr.Character
									Plr.Character:MoveTo(OldPos)
									Tap = false
								end
								if Plr.Character then
									Apply()
								end
							end
						end
					end)

					repeat wait() until Plr.Character

					local Clean

					Clean = game.Players.LocalPlayer.CharacterAdded:connect(function(char)
						Touched:Disconnect()
						Platform:Destroy()
						Clean:Disconnect()
					end)

					OldPos = Plr.Character.HumanoidRootPart.Position
					Plr.Character:MoveTo(Platform.Position + Vector3.new(0,0.5,0))

				end,

			},

			{

				Name = "Get Blocks",
				Type = "Toggle",
				Desc = "Get all the blocks that have the cooldown off.",
				Action = function(On)

					if On then

						Collecting = true

						for _, Thing in game.Workspace:GetDescendants() do

							if Thing.Name == "TouchMe" and Thing:IsA("BasePart") then

								Thing:SetAttribute("Pos",Thing.Position)

