			},

			{

				Name = "Speed Type",
				Desc = "Lets you pick teleport or normal speed, Teleport lets you move while ragdolled.",
				Type = "Select",
				Options = {"Normal","Teleport"},
				Action = function(Pick)

					SpeedType = Pick

				end,

			},

			{

				Name = "Toggle Speed",
				Desc = "Toggles the speed.",
				Type = "Toggle",
				Action = function(On)

					if On then

						SpeedOn = true

						spawn(function()

							while SpeedOn do

								wait()

								if SpeedType == "Normal" then

									Plr.Character.Humanoid.WalkSpeed = SpeedAmnt

								else

									Plr.Character.Humanoid.WalkSpeed = 16

									local Delta = game["Run Service"].Heartbeat:Wait()

									Plr.Character:TranslateBy(Plr.Character.Humanoid.MoveDirection * Delta * SpeedAmnt)

								end

							end

						end)

					else

						SpeedOn = false
						wait()
						Plr.Character.Humanoid.WalkSpeed = 16

					end

				end,

			},

			{

				Type = "Split",

			},

			{

				Name = "Climb Walls",
				Desc = "Allows you to climb everything.",
				Type = "Toggle",
				Action = function(On)

					if On then

						local Players = game:GetService("Players")
						local RunService = game:GetService("RunService")

						Truss = Instance.new("TrussPart",game.Workspace)
						Truss.Anchored = true
						Truss.Size = Vector3.new(0.4,2048,0.4)
						Truss.Color = Color3.new(1,0,0)
						Truss.Material = Enum.Material.Neon
						Truss.Name = "RayTruss"
						Truss.CanTouch = false
						Truss.Transparency = 1

						local Params = RaycastParams.new()
						Params.FilterType = Enum.RaycastFilterType.Exclude
						Params.FilterDescendantsInstances = {Truss,Plr.Character}

						WallClimb = RunService.RenderStepped:Connect(function()

							if not Plr.Character.HumanoidRootPart or not Plr.Character.HumanoidRootPart.Parent then return end

							local Result = workspace:Raycast(Plr.Character.HumanoidRootPart.Position - Vector3.new(0,2,0), Plr.Character.HumanoidRootPart.CFrame.LookVector * 2.5, Params)

							if Result and ClimbUse then
								Truss.Position = Result.Position
							else
								Truss.Position = Vector3.new(9999999,9999999,9999999)
								ClimbUse = false
								wait(0.3)
								ClimbUse = true
							end

						end)

					else

						WallClimb:Disconnect()
						Truss:Destroy()

					end

				end,

			},

			{

				Type = "Title",
				Text = "Target"

			},

			{

				Name = "Quick Kill Target",
				Desc = "Kills the target but don't count as a kill. (Your not the person who killed them)",
				Type = "Button",
				Action = function()

					if not Target or not Target:FindFirstChildOfClass("Humanoid") then
						
						print(Target)
						ShowPopup("Error","Please press T on a player to select as target.","Got it")
						return

					end
						
					if Killing then

						ShowPopup("Error","Your already killing someone.","Right")
						return

					end

					if Target:FindFirstChild("PVPOFFGUI") and Target:FindFirstChild("PVPOFFGUI").TextLabel == "" then

						ShowPopup("Error","This player doesn't have PVP on.","Ok")
						return

					end

					if Plr.jaladaDePeloCharge.Value ~= 100 then

						ShowPopup("Error","Your blue bar (Hair grabbing bar) needs to be full for this.","Got it")
						return

					end

					Killing = true

					if not game.Workspace:FindFirstChild("FloatKill") then

						local Float = Instance.new("Part",game.Workspace)

						Float.Position = Vector3.new(0,-1000,0)
						Float.Name = "FloatKill"
						Float.Size = Vector3.new(2048,1,2048)
						Float.Transparency = 1
						Float.Anchored = true

					end

					local OldPos = Plr.Character.HumanoidRootPart.CFrame

					Plr.Character.HumanoidRootPart.CFrame = CFrame.new(0,-996.5,0)

					game.Workspace.FallenPartsDestroyHeight = -10000

					repeat

						wait()

						game:GetService("ReplicatedStorage").JALADADEPELOEVENT:FireServer()

						Plr.Character.HumanoidRootPart.CFrame = CFrame.new(0,-996.5,0)

						Target.LeftFoot:BreakJoints()
						Target.LeftFoot.CanCollide = false
						Target.LeftFoot.Position = Plr.Character.Humanoid.RootPart.Position + (Plr.Character.Humanoid.RootPart.CFrame.LookVector * 2)
						Target.LeftFoot.Velocity = Vector3.new(0,0,0)

					until Target:FindFirstChild("BeingCarried")

					repeat

						wait()

					until not Target:FindFirstChild("BeingCarried")

					wait(0.1)

					Plr.Character.HumanoidRootPart.CFrame = OldPos

					Killing = false

				end,

			},

			{

				Name = "Kill Target",
				Desc = "Kills the target normally.",
				Type = "Button",
				Action = function()

					if not Target then

						ShowPopup("Error","Please press T on a player to select as target.","Got it")
						return

					end

					if Killing then

						ShowPopup("Error","Your already killing someone.","Right")
						return

					end

					if Target:FindFirstChild("PVPOFFGUI") and Target:FindFirstChild("PVPOFFGUI").TextLabel == "" then

						ShowPopup("Error","This player doesn't have PVP on.","Ok")
						return

					end

					Killing = true

					if not game.Workspace:FindFirstChild("FloatKill") then

						local Float = Instance.new("Part",game.Workspace)

						Float.Position = Vector3.new(0,-1000,0)
						Float.Name = "FloatKill"
						Float.Size = Vector3.new(2048,1,2048)
						Float.Transparency = 1
						Float.Anchored = true

					end

					local OldPos = Plr.Character.HumanoidRootPart.CFrame

					Plr.Character.HumanoidRootPart.CFrame = CFrame.new(0,-996.5,0)

					game.Workspace.FallenPartsDestroyHeight = -10000

					game.Workspace.CurrentCamera.CameraSubject = Target.Humanoid

					repeat

						wait()

						if Plr.Backpack:FindFirstChild("1 punches") then

							Plr.Character.Humanoid:EquipTool(Plr.Backpack["1 punches"])

						end

						game:GetService("ReplicatedStorage"):WaitForChild("PUNCHEVENT"):FireServer(1)

						Plr.Character.HumanoidRootPart.CFrame = CFrame.new(0,-996.5,0)

						Target.LeftFoot:BreakJoints()
						Target.LeftFoot.CanCollide = false
						Target.LeftFoot.Position = Plr.Character.Humanoid.RootPart.Position + (Plr.Character.Humanoid.RootPart.CFrame.LookVector * 2)
						Target.LeftFoot.Velocity = Vector3.new(0,0,0)

					until Target:FindFirstChild("canBeStomped") or not Target or Plr.Character.Humanoid.Health == 0

					if not Target or Plr.Character.Humanoid.Health == 0 then

						Plr.Character.HumanoidRootPart.CFrame = OldPos

						Killing = false

						game.Workspace.CurrentCamera.CameraSubject = Plr.Character.Humanoid

						return

					end

					repeat

						wait()

						if Plr.Backpack:FindFirstChild("1 punches") then

							Plr.Character.Humanoid:EquipTool(Plr.Backpack["1 punches"])

						end

						game:GetService("ReplicatedStorage"):WaitForChild("STOMPEVENT"):FireServer()

						Plr.Character.HumanoidRootPart.CFrame = Target.HumanoidRootPart.CFrame + Vector3.new(0,5,0)

					until Target.Humanoid.Health == 0 or not Target or Plr.Character.Humanoid.Health == 0

					if not Target or Plr.Character.Humanoid.Health == 0 then

						Plr.Character.HumanoidRootPart.CFrame = OldPos

						game.Workspace.CurrentCamera.CameraSubject = Plr.Character.Humanoid

						Killing = false

						return

					end

					wait(0.1)

					Plr.Character.HumanoidRootPart.CFrame = OldPos

					Killing = false

					game.Workspace.CurrentCamera.CameraSubject = Plr.Character.Humanoid

				end,

			},
			
			{

				Name = "Bring Target",
				Desc = "Brings the person to you. You need a full blue bar for this.",
				Type = "Button",
				Action = function()

					if not Target or not Target:FindFirstChildOfClass("Humanoid") then

						ShowPopup("Error","Please press T on a player to select as target.","Got it")
						return

					end

					if Target:FindFirstChild("PVPOFFGUI") and Target:FindFirstChild("PVPOFFGUI").TextLabel == "" then

						ShowPopup("Error","This player doesn't have PVP on.","Ok")
						return

					end

					if Plr.jaladaDePeloCharge.Value ~= 100 then

						ShowPopup("Error","Your blue bar (Hair grabbing bar) needs to be full for this.","Got it")
						return

					end

					repeat

						wait()

						game:GetService("ReplicatedStorage").JALADADEPELOEVENT:FireServer()

						Target.LeftFoot:BreakJoints()
						Target.LeftFoot.CanCollide = false
						Target.LeftFoot.Position = Plr.Character.Humanoid.RootPart.Position + (Plr.Character.Humanoid.RootPart.CFrame.LookVector * 2)
						Target.LeftFoot.Velocity = Vector3.new(0,0,0)

					until Target:FindFirstChild("BeingCarried")

				end,

			},
			
			{

				Type = "Title",
				Text = "Other"

			},
			
			{

				Name = "Throwable Giver",
				Desc = "Always gives you something to throw.",
				Type = "Toggle",
				Action = function(On)

					if On then
						
						ItemGiving = true
						
						spawn(function()
							
							while ItemGiving do

								wait(0.02)

								if not Plr.Character:FindFirstChild("Trash Can") and not Plr.Character:FindFirstChild("Bench") and not Plr.Character:FindFirstChild("Chair") then

									if not Plr.Backpack:FindFirstChild("Trash Can") and not Plr.Backpack:FindFirstChild("Bench") and not Plr.Backpack:FindFirstChild("Chair") then

										for _, Item in game.Workspace:GetDescendants() do

											if Item.Name == "Trash Can" or Item.Name == "Park Bench" or Item.Name == "Chair" then
												
												if Item:IsA("BasePart") and Item:FindFirstChildOfClass("ProximityPrompt") and Item:FindFirstChildOfClass("ProximityPrompt").MaxActivationDistance ~= 0 then

													local GotIt = false

													local OldPos = Plr.Character.Head.CFrame
													local OldPos2 = Item.CFrame

													Item:FindFirstChildOfClass("ProximityPrompt").MaxActivationDistance = 1000000
													
													game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Scriptable
													
													spawn(function()
														
														while not GotIt do

															Plr.Character.HumanoidRootPart.CFrame = OldPos2
															Item:FindFirstChildOfClass("ProximityPrompt"):InputHoldBegin()
															wait(0.01)
															Item:FindFirstChildOfClass("ProximityPrompt"):InputHoldEnd()
															wait()

														end
														
													end)
													
													wait(0.2)

													Item.CFrame = game.Workspace.CurrentCamera.CFrame + game.Workspace.CurrentCamera.CFrame.LookVector * 20

													repeat
														
														wait()
														
													until Item:FindFirstChildOfClass("ProximityPrompt").MaxActivationDistance == 0

													GotIt = true

													wait(0.01)
													
													spawn(function()
														
														for i=1,5 do
															
															wait(0.01)
															Plr.Character.HumanoidRootPart.CFrame = OldPos
															game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Track
															
														end
														
													end)
													Item.CFrame = OldPos2
													
													break

												end

											end

										end

									end

								end

							end
							
						end)
						
					else
						
						ItemGiving = false
						
					end

				end,

			},
			
		}

	end,
	
