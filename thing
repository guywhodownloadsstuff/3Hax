			{

				Name = "Creative Mode (OP)",
				Type = "Button",
				Desc = "Makes you basically a god. You can mess around with players and go in freecam mode. While staying in your forcefield to be immortal.",
				Action = function()

					if game.Workspace:FindFirstChild("Freecam") then

						game.Workspace:FindFirstChild("Freecam"):Destroy()

					end

					local Ok = true

					for _, Person in game.Players:GetPlayers() do

						if Person.Character and Person.Character:FindFirstChildOfClass("Humanoid") then

							pcall(function()

								Person.Character:FindFirstChildOfClass("Humanoid").DisplayDistanceType = Enum.HumanoidDisplayDistanceType.Subject
								Person.Character:FindFirstChildOfClass("Humanoid").HealthDisplayDistance = math.huge
								Person.Character:FindFirstChildOfClass("Humanoid").NameDisplayDistance = math.huge

							end)

						end

						Person.CharacterAppearanceLoaded:Connect(function()

							if Ok then

								pcall(function()

									Person.Character:FindFirstChildOfClass("Humanoid").DisplayDistanceType = Enum.HumanoidDisplayDistanceType.Subject
									Person.Character:FindFirstChildOfClass("Humanoid").HealthDisplayDistance = math.huge
									Person.Character:FindFirstChildOfClass("Humanoid").NameDisplayDistance = math.huge

								end)

							end

						end)

					end

					if (mouse1press and mouse1release) == nil then

						ShowPopup("Creative Mode","Sadly your executor dont support autoclicking. You must manually click over and over to use these items. (Some commands like kill dont require manual clicking.)","Aww")

					else

						ShowPopup("Creative Mode","Press backspace to stop trying to do a command on somebody.","Got it")

					end

					local FreeMove = false

					local Cooldown = false

					local Mode = "None"

					local Look = Instance.new("Part",game.Workspace)

					local ClickCheck = Mouse.Button1Down:Connect(function()

						if Cooldown then

							return

						end

						if Mode == "None" then

							ShowPopup("Creative Mode","Please select a tool.","Ok")
							return

						end
						if Mouse.Target and Mouse.Target.Parent:FindFirstChild("HumanoidRootPart") or Mouse.Target and Mouse.Target.Parent.Parent:FindFirstChild("HumanoidRootPart") then
							if Mode == "Kill" and Mouse.Target.Parent:FindFirstChild("HumanoidRootPart") or Mode == "Kill" and Mouse.Target.Parent.Parent:FindFirstChild("HumanoidRootPart") then

								if not Plr.Backpack:FindFirstChild("TriLaserGun") then

									ShowPopup("Creative Mode","'TriLaserGun' Required. Wait abit for the game to give you the item or keep resetting until this works frequently.","Got it")
									return

								end

								local Character = nil

								if Mouse.Target.Parent:FindFirstChild("HumanoidRootPart") then

									Character = Mouse.Target.Parent

								else

									Character = Mouse.Target.Parent.Parent

								end

								local Outline = Instance.new("Highlight",Character)

								Outline.OutlineColor = Color3.new(1,0,0)
								Outline.OutlineTransparency = 1
								Outline.FillTransparency = 1
								Outline.Name = "DeathLine"

								game.TweenService:Create(Outline,TweenInfo.new(0.3),{OutlineTransparency = 0}):Play()

								Cooldown = true

								Plr.Character.Humanoid:EquipTool(Plr.Backpack:FindFirstChild("TriLaserGun"))

								local Cancel = false

								repeat

									task.wait()
									Character["Left Leg"].Size = Vector3.new(20,math.random(3,6),20)
									Character["Left Leg"]:BreakJoints()
									Character["Left Leg"].Position = Plr.Character.Head.Position + Vector3.new(0,math.random(4,20),0)
									Character["Left Leg"].Velocity = Vector3.new(0,0,0)

									spawn(function()
										if Plr.Character:FindFirstChild("TriLaserGun") and Plr.Character:FindFirstChild("TriLaserGun").Enabled == true then
											Plr.Character.TriLaserGun.Click:FireServer(Character["Left Leg"].Position)
										end
									end)

								until Character.Humanoid.Health == 0 or UIS:IsKeyDown(Enum.KeyCode.Backspace)

								Cancel = true

								Cooldown = false

								Outline.Name = "Finished"

								game.TweenService:Create(Outline,TweenInfo.new(0.3),{OutlineTransparency = 1}):Play()

								Plr.Character.Humanoid:UnequipTools()

								wait(0.3)

								Outline:Destroy()

							end

							if Mode == "Fling" and Mouse.Target.Parent:FindFirstChild("HumanoidRootPart") or Mode == "Fling" and Mouse.Target.Parent.Parent:FindFirstChild("HumanoidRootPart") then

								if not Plr.Backpack:FindFirstChild("Windforce") then

									ShowPopup("Creative Mode","'Windforce' Required. Wait abit for the game to give you the item or keep resetting until this works frequently.","Got it")
									return

								end

								local Character = nil

								if Mouse.Target.Parent:FindFirstChild("HumanoidRootPart") then

									Character = Mouse.Target.Parent

								else

									Character = Mouse.Target.Parent.Parent

								end

								local Outline = Instance.new("Highlight",Character)

								Outline.OutlineColor = Color3.new(1,0,0)
								Outline.OutlineTransparency = 1
								Outline.FillTransparency = 1
								Outline.Name = "DeathLine"

								game.TweenService:Create(Outline,TweenInfo.new(0.3),{OutlineTransparency = 0}):Play()

								Cooldown = true

								Plr.Character.Humanoid:EquipTool(Plr.Backpack:FindFirstChild("Windforce"))

								local Cancel = false

								local OldHealth = Character.Humanoid.Health

								repeat

									task.wait()
