	["662417684"] = function ()

		local Opening = false
		local Collecting = false
		local OldPos = Plr.Character.HumanoidRootPart.Position

		Commands = {

			{

				Type = "Title",
				Text = "Blocks"

			},

			{

				Type = "Split",

			},

